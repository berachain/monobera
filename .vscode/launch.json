{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm dev",
      "cwd": "${workspaceFolder}/apps/dapp/",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "debug dapp",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/apps/dapp/",
      "trace": true
    }
  ]
}
