{
  "name": "monobera",
  "private": true,
  "engines": {
    "node": ">=v18.16.0"
  },
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "storybook": "turbo storybook",
    "storybook:build": "turbo storybook:build",
    "build": "turbo build",
    "build:force": "turbo build --force",
    "clean": "turbo clean",
    "clean:git": "git clean -xdf node_modules",
    "dev": "turbo dev --concurrency=12",
    "dev:dex": "turbo dev --filter=dex",
    "dev:honey": "turbo dev --filter=honey --filter='./packages/*'",
    "dev:bgt": "turbo dev --filter=bgt-station --filter='./packages/*'",
    "dev:lend": "turbo dev --filter=lend --filter='./packages/*'",
    "dev:perp": "turbo dev --filter=perp --filter='./packages/*'",
    "dev:faucet": "turbo dev --filter=faucet --filter='./packages/*'",
    "check-types": "turbo check-types",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "lint": "turbo lint && manypkg check",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@bera/eslint-config": "0.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@types/prettier": "^2.7.2",
    "eslint": "^8.39.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "turbo": "^1.10.15",
    "typescript": "^5.0.4"
  }
}
