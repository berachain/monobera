{
  "name": "monobera",
  "private": true,
  "engines": {
    "node": ">=v18.16.0"
  },
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "storybook": "turbo storybook",
    "storybook:build": "turbo storybook:build",
    "build": "turbo build",
    "clean": "turbo clean && git clean -xdf node_modules",
    "clean:git": "git clean -xdf node_modules",
    "pullenv": "vercel env pull --environment=production",
    "dev": "turbo dev --concurrency=19",
    "build:dex": "turbo build --filter=dex --filter='./packages/*' --concurrency=19",
    "dev:dex": "turbo dev --filter=dex",
    "dev:honey": "turbo dev --filter=honey --filter='./packages/*' --concurrency=19",
    "dev:bgt": "turbo dev --filter=bgt-station --filter='./packages/*' --concurrency=19",
    "dev:lend": "turbo dev --filter=lend --filter='./packages/*' --concurrency=19",
    "dev:perp": "turbo dev --filter=perp --filter='./packages/*' --concurrency=19",
    "dev:faucet": "turbo dev --filter=faucet --filter='./packages/*' --concurrency=19",
    "dev:clue": "turbo dev --filter=validator-clue --filter='./packages/*' --concurrency=19",
    "dev:ambassador": "turbo dev --filter=ambassador --filter='./packages/*' --concurrency=19",
    "dev:eccsystem": "turbo dev --filter=ecosystem --filter='./packages/*' --concurrency=19",
    "check-types": "turbo check-types",
    "lint": "biome lint . ",
    "lint:apply": "biome lint . --apply",
    "lint:apply-unsafe": "biome lint . --apply-unsafe",
    "format:check": "biome format .",
    "format": "biome format --write .",
    "check": "biome check .",
    "prepare": "husky install",
    "upsertenv": "node ./scripts/upsertVercelEnv.js"
  },
  "dependencies": {
    "@bera/eslint-config": "0.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@types/prettier": "^2.7.2",
    "eslint": "^8.39.0",
    "fetch-mock": "^9.11.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "turbo": "^1.10.15",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "yargs": "^17.7.2"
  }
}
